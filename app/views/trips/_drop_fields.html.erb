<div class="nested-fields">

      <div class="row">
        
        <div class="form-group col-md-1">
          <%= f.label :drop_number, "Drop Number", class: "col-md-1 control-label" %>
          <%= f.text_field :drop_number, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :lines, "Lines Fished", class: "col-md-1 control-label" %>
          <%= f.text_field :lines, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :start_time, "Start Time", class: "col-md-1 control-label" %>
          <%= f.time_field :start_time, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :start_depth, "Start Depth", class: "col-md-1 control-label" %>
          <%= f.text_field :start_depth, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :start_lat, "Start Lat", class: "col-md-1 control-label" %>
          <%= f.text_field :start_lat, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :start_lon, "Start Lon", class: "col-md-1 control-label" %>
          <%= f.text_field :start_lon, class: "form-control" %>
        </div>
        
        <div class="form-group col-md-1">
          <%= f.label :end_time, "End Time", class: "col-md-1 control-label" %>
          <%= f.time_field :end_time, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :end_depth, "End Depth", class: "col-md-1 control-label" %>
          <%= f.text_field :end_depth, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :end_lat, "End Lat", class: "col-md-1 control-label" %>
          <%= f.text_field :end_lat, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :end_lon, "End Lon", class: "col-md-1 control-label" %>
          <%= f.text_field :end_lon, class: "form-control" %>
        </div>

        <div class="form-group col-md-1">
          <%= f.label :fish_caught, "Fish Caught?", class: "col-md-1 control-label" %>
          <%= f.text_field :fish_caught, class: "form-control" %>
        </div>
        
        <%= link_to_remove_association "remove drop", f %>

      </div>


</div>
