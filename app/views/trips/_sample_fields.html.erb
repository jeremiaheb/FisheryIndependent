<div class="nested-fields">
  
  Grid Number: <%= f.text_field :grid_number %>
  Arrival Time: <%= f.time_field :arrival_time %>
  Wind Speed: <%= f.text_field :wind_speed %>
  Wave Height: <%= f.text_field :wave_height %>
  Current Speed: <%= f.text_field :current_speed %>
  Sample Notes: <%= f.text_field :sample_notes %>


    <%= f.fields_for :drops do |builder| %>
    <%= render 'drop_fields', f: builder %>
    <% end %>
    <div class="links">
    <%= link_to_add_association "Add Drop",f,:drops %>
    </div>
  
  <%= link_to_remove_association "remove sample", f %>

</div>
